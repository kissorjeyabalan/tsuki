{
  "assignees": ["kissorjeyabalan"],
  "baseBranches": ["develop"],
  "extends": ["config:base", ":dependencyDashboard"],
  "rebaseWhen": "behind-base-branch",
  "reviewers": ["kissorjeyabalan"],
  "packageRules": [
    {
      "groupName": "compose",
      "matchPackagePrefixes": ["androidx.compose"]
